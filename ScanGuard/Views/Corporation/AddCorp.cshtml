@model CorporationEntity

<h2>Add Corporation</h2>
@if (TempData["ErrorMessage"] != null)
{
    <div class="alert alert-danger">@TempData["ErrorMessage"]</div>
}
<form asp-action="AddCorp" method="post">
    <input asp-for="CorpName" placeholder="Corporation Name" />
    <input asp-for="Website" placeholder="Website" />
    <input asp-for="Email" placeholder="Email" />
    <input asp-for="Phone" placeholder="Phone" />
    <input asp-for="Address" placeholder="Address" />
    <input asp-for="City" placeholder="City" />
    <input asp-for="State" placeholder="State" />
    <input asp-for="Zip" placeholder="Zip" />
    <input asp-for="Country" placeholder="Country" />
    <input asp-for="WorkerCount" placeholder="Worker Count" type="number" />
    <button type="submit">Submit</button>
</form>
