@model string

@if (!String.IsNullOrEmpty(Model))
{
    var statusMessageClass = Model.StartsWith("Error") ? "danger" : "success";
    <div style="margin-bottom:0px;margin-top:0px; margin-left:20px; margin-right:20px;" class="alert alert-@statusMessageClass alert-dismissible" role="alert">

        
        @Model
        <a asp-page="./Login">Go to Log In page</a>
    </div>
}
